{
  "name": "t4-backend",
  "version": "0.1",
  "description": "T4 App API",
  "main": "index.js",
  "repository": "",
  "author": "yev@t4.ai",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && mkdir dist && ./node_modules/babel-cli/bin/babel.js --copy-files src -d dist",
    "test": "./node_modules/mocha/bin/mocha --require babel-polyfill --require babel-register --exit \"./test/**/*.test.js\" -r chai/register-assert",
    "start": "nodemon src/index.js --exec 'node_modules/.bin/eslint src && node'",
    "serve": "yarn run build && node ./dist/index.js",
    "fix-code": "node_modules/.bin/prettier \"./src/**/*.js\" --write",
    "sq": "export $(grep -v '^#' .env | xargs) && ./node_modules/.bin/sequelize",
    "db:setup": "yarn run sq db:create && yarn run sq db:migrate",
    "db:reset": "yarn run sq db:drop && yarn run sq db:create && yarn run sq db:migrate",
    "db:migrate": "yarn run sq db:migrate",
    "db:migration:create": "yarn run sq migration:create",
    "dashboards:update": "export $(grep -v '^#' .env | xargs) && node --require babel-polyfill --require babel-register ./src/scripts/updateDashboard.js",
    "reportData:reload": "export $(grep -v '^#' .env | xargs) && node --require babel-polyfill --require babel-register ./src/scripts/reportImportScript.js",
    "reportData:checkUrls": "export $(grep -v '^#' .env | xargs) && node --require babel-polyfill --require babel-register ./src/scripts/checkReportUrls.js",
    "companyData:reload": "export $(grep -v '^#' .env | xargs) && node --require babel-polyfill --require babel-register ./src/scripts/companyImportScript.js",
    "industryData:reload": "export $(grep -v '^#' .env | xargs) && node --require babel-polyfill --require babel-register ./src/scripts/industryImportScript.js",
    "allData:reload": "yarn reportData:reload && yarn industryData:reload && yarn companyData:reload",
    "users:create": "node --require babel-polyfill --require babel-register ./src/scripts/createUsers.js",
    "sandbox": "export $(grep -v '^#' .env | xargs) && node --require babel-polyfill --require babel-register ./sandbox.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "csvtojson": "^2.0.8",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "@sentry/node": "5.5.0",
    "amplitude": "^3.5.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "hubspot": "^2.1.3",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.19",
    "morgan": "^1.9.0",
    "ms": "^2.1.1",
    "owasp-password-strength-test": "^1.3.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.5.0",
    "sequelize": "^4.40.0",
    "sequelize-cli": "^5.1.0",
    "winston": "^3.1.0"
  }
}
